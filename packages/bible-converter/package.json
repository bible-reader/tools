{
  "name": "@scripture-app/bible-converter",
  "version": "0.6.6",
  "description": "Library to convert Bibles to JSON. Written in TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bible-reader/tools.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["bible", "xml", "json", "opensong"],
  "author": "Pavel Struhar",
  "license": "MIT",
  "dependencies": {
    "@scripture-app/common": "^0.5.0",
    "@scripture-app/types": "^0.6.6",
    "@types/fs-promise": "^1.0.3",
    "@types/node": "^8.0.2",
    "@types/xml-parser": "^1.2.28",
    "fs-promise": "^1.0.0",
    "xml-parser": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "jest": "^22.0.4",
    "prettier": "^1.4.4",
    "ts-jest": "^22.0.1",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.3.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
