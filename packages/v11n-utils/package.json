{
  "name": "@bible-reader/v11n-utils",
  "version": "2.0.0",
  "description": "Utility library for working with v11n (versification) data object",
  "repository": {
    "type": "git",
    "url": "https://github.com/bible-reader/tools.git"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "tsc -p tsconfig.build.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Pavel Struhar",
  "license": "MIT",
  "dependencies": {
    "@bible-reader/types": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^2.6.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
